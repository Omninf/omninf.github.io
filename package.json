{
  "name": "omni-css.github.io",
  "type": "module",
  "private": true,
  "engines": {
    "bun": ">=1.0"
  },
  "packageManager": "bun@1.0.14",
  "scripts": {
    "astro": "astro",
    "build": "bun check && astro build",
    "check": "biome ci . && astro check",
    "check.format": "biome format .",
    "check.lint": "biome check .",
    "dev": "astro dev",
    "fix": "bun lint && bun format",
    "format": "biome format --write .",
    "inspect": "node --inspect-brk ./node_modules/vite/bin/vite.js --force",
    "lint": "biome check --apply-unsafe .",
    "preview": "astro preview",
    "start": "astro dev --open"
  },
  "dependencies": {
    "@astrojs/check": "^0.3.1",
    "@astrojs/starlight": "^0.14.0",
    "@astrojs/starlight-tailwind": "^2.0.1",
    "@astrojs/tailwind": "^5.0.2",
    "@builder.io/qwik": "^1.2.19",
    "@qwikdev/astro": "^0.2.8",
    "astro": "^3.2.3",
    "sharp": "^0.33.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "autoprefixer": "^10.4.16",
    "bun-types": "^1.0.14",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "postcss-load-config": "^4.0.2",
    "postcss-nested": "^6.0.1"
  },
  "trustedDependencies": [
    "sharp"
  ]
}